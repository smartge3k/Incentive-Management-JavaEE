<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
                 version="1.0">
    <named-query name="getallbeverages">
        <query>SELECT b FROM BeverageEntity b </query>
    </named-query>

     <named-query name="getallincentives">
        <query>SELECT i FROM IncentiveEntity i </query>
    </named-query>
    
    <named-query name="getallorders">
        <query>SELECT c FROM IncentiveEntity c </query>
    </named-query>


</entity-mappings>
